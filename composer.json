{
    "name": "atlasman/laravel-callback",
    "description": "http callback request for laravel",
    "license": "MIT",
    "keywords": ["laravel", "callback"],
    "authors": [
        {
            "name": "atlasman",
            "email": "iatlasman@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/container": "~5|~6",
        "illuminate/http": "~5|~6",
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^7.5 ",
        "mockery/mockery": "1.3.0"
    },
    "autoload": {
        "psr-4": {
            "Atlasman\\LaravelCallback\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atlasman\\LaravelCallback\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Atlasman\\LaravelCallback\\ServiceProvider"
            ]
        }
    }
}